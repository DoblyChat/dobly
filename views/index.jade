#login
	#prompt Sign In to Fluidtalk			
	#form
		form(method='post', action="/login")					
			input#username.field(type="text", name="username", placeholder="Username")
			input#password.field(type="password", name="password", placeholder="Password")
			input#sign-in(type="submit", value="Sign In")
		if showFlash
			#flash= info
